{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "importHelpers": true,
    "strict": true,
    "jsx": "react",
    "noEmitHelpers": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "types": ["react-dom", "node"],
    "baseUrl": "types",
    "typeRoots": [
      "types"
]
  },
      "include": [
          "**/*.ts",
      ],
      "exclude": [
        "**/*.spec.ts",
      ]
}
